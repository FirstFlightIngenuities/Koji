			while (!!linesDrawnSoFar[ind]) ind = Math.floor(Math.random() * (width+1));
			linesDrawnSoFar[ind] = 1;
			numberOfLinesDrawnOnCanvas++;
		} else {
			var ind;
			for (var i = 0; i < Math.floor(width); i++) {
				if (!linesDrawnSoFar[i]) {ind = i; break;}
			}
			linesDrawnSoFar[ind] = 1;
			numberOfLinesDrawnOnCanvas++;				
		}
