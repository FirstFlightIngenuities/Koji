	<div class="signup-form-div">
		<%= form_tag("/", method: "", id: "signupForm", class: "signup-form-form") do %>
			<p class="signup-form-paragraph" style="font-size: 0.9em;">Email and cellphone will be used for login and password recovery. Sign-up requires a $2 (two dollars) recurring monthly membership fee.</p>
			<p class="signup-form-paragraph">&nbsp;</p>
			<p class="signup-form-paragraph"><span id="emailErrorMessage" style="color:red"></span></p>
			<p class="signup-form-paragraph"><input id="emailInput" class="signup-form-input" type="text" name="email" placeholder=" Email" maxlength="70" onkeyup="validateEmail(this,1)" onblur="validateEmail(this,1)"><%= image_tag "greenCheck.png", :style => "width: 1.2em; height: 1.2em; margin-left: 0.5em; visibility: hidden;" %></p>
			<p class="signup-form-paragraph" style="font-size: 0.8em;">- Cellphone optional -</p>
			<p class="signup-form-paragraph"><span id="cellphoneErrorMessage" style="color:red"></span></p>
			<p class="signup-form-paragraph"><input id="numberInput" style="font-size: 1em; margin: 0;" type="number" name="none" onkeyup="validateCellphone(document.getElementById('cellphoneInput'),1)"></p>
			<p class="signup-form-paragraph" style="margin-top: -2em;"><input id="cellphoneInput" class="signup-form-input" type="text" name="cell" placeholder=" Cellphone" maxlength="20" onkeyup="validateCellphone(this,1)" onfocus="validateCellphone(this,1)"><%= image_tag "greenCheck.png", :style => "width: 1.2em; height: 1.2em; margin-left: 0.5em; visibility: hidden;" %></p>
			<p class="signup-form-paragraph"><span id="displaynameErrorMessage" style="color:red"></span></p>
			<p class="signup-form-paragraph"><input id="displaynameInput" class="signup-form-input" type="text" name="displayname" placeholder=" Display Name" maxlength="70" onkeyup="validateDisplayName(this)" onblur="validateDisplayName(this)"><%= image_tag "greenCheck.png", :style => "width: 1.2em; height: 1.2em; margin-left: 0.5em; visibility: hidden;" %></p>
			<p class="signup-form-paragraph"><span id="password1ErrorMessage" style="color:red"></span></p>
			<p class="signup-form-paragraph"><input id="password1Input" class="signup-form-input" type="password" name="password1" placeholder=" Password" maxlength="70" onkeyup="validatePassword()" onblur="validatePassword()"><%= image_tag "greenCheck.png", :style => "width: 1.2em; height: 1.2em; margin-left: 0.5em; visibility: hidden;" %></p>
			<p class="signup-form-paragraph"><span id="password2ErrorMessage" style="color:red"></span></p>
			<p class="signup-form-paragraph"><input id="password2Input" class="signup-form-input" type="password" name="password2" placeholder=" Re-enter Password" maxlength="70" onkeyup="validatePassword()" onblur="validatePassword()"><%= image_tag "greenCheck.png", :style => "width: 1.2em; height: 1.2em; margin-left: 0.5em; visibility: hidden;" %></p>
			<p class="signup-form-paragraph"></p>
			<p class="buttons-paragraph"><input id="signupSubmit" class="signup-form-submit-button" type="button" name="signupSubmit" value="Payment Info >" onclick="stripePopup()" onmousedown="if (!this.disabled) this.style.boxShadow='none'" onmouseup="if (!this.disabled) this.style.boxShadow='-1px 3px 9px #777777'" disabled="disabled"></p>
		<% end %>
		<div class="form-footer"></div>
	</div>
	<div class="modal-div-footer"></div>
	<div class="hidden-opt-left" onclick="closeMenu(document.getElementById('signupDiv')); showMenu(document.getElementById('menuDiv'));"></div>
	<div class="hidden-opt-right" onclick="closeMenu(document.getElementById('signupDiv'))"></div>
</div><!-- end signupDiv -->



